<UserControl x:Class="Uplink2.PreviousAgents"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Uplink2"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="150" MaxWidth="150" Width="150">
    <Grid Background="OrangeRed">
        <Grid.ColumnDefinitions>
            <ColumnDefinition MaxWidth="150" Width="150"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="30"></RowDefinition>
            <RowDefinition Height="Auto"></RowDefinition>
        </Grid.RowDefinitions>
        <Label Grid.Column="0" Grid.Row="0" Content="Previous Logins:" Foreground="White" Background="DarkRed" MaxWidth="150" Width="150"></Label>
        <DataGrid Grid.Row="1" Background="DarkRed" Foreground="White" AutoGenerateColumns="False" CanUserAddRows="True" CanUserDeleteRows="True" CanUserReorderColumns="False" CanUserResizeColumns="False" CanUserResizeRows="False" CanUserSortColumns="True" AlternatingRowBackground="DarkRed" AlternationCount="1" MouseDoubleClick="DataGrid_MouseDoubleClick"> 
        </DataGrid>
    </Grid>
</UserControl>
